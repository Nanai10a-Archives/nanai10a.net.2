{
  "name": "www.nanai10a.net",
  "version": "1.0.0-alpha",
  "repository": "https://github.com/Nanai10a/www.nanai10a.net.git",
  "scripts": {
    "prepare:husky": "husky install",
    "prepare": "run-s prepare:husky",
    "run:dev": "env-cmd -e dev next dev",
    "run:prod": "env-cmd -e prod next start",
    "dev": "run-s run:dev",
    "start": "run-s run:prod",
    "build": "next build",
    "lint": "eslint -c .eslintrc.yml --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "eslint -c .eslintrc.yml --fix --ext .ts,.tsx,.js,.jsx ."
  },
  "author": "Nanai10a <Nanai10a@outlook.jp>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "next": "^12.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "<8.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4",
    "yarn": "^1.22.17"
  },
  "private": true
}
